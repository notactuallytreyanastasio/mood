//DOOMAI2  JOB (ACCT),'DOOM AI V2',CLASS=A,MSGCLASS=X,
//         MSGLEVEL=(1,1),REGION=0M,NOTIFY=&SYSUID
//*
//* DOOM AI Version 2 - Enhanced AI with State Processing
//* Reads game state from FTP'd dataset and writes commands
//*
//FTPGET   EXEC PGM=FTP,PARM='(EXIT'
//STEPLIB  DD DSN=TCPIP.SEZALINK,DISP=SHR
//SYSPRINT DD SYSOUT=*
//OUTPUT   DD SYSOUT=*
//INPUT    DD *
open 192.168.1.100 21
user doom doomguy
cd /doom/gamestate
lcd 'DOOM.GAMESTAT'
get GAMESTAT.CURRENT 'DOOM.GAMESTAT'
quit
/*
